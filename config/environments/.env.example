# SalesBoost Environment Variables

PROJECT_NAME=SalesBoost
VERSION=1.0.0
DESCRIPTION=Multi-agent sales coaching platform

ENV_STATE=development
DEBUG=true

HOST=0.0.0.0
PORT=8000
CORS_ORIGINS=["*"]

DATABASE_URL=sqlite+aiosqlite:///./salesboost.db
REDIS_URL=redis://localhost:6379/0
LOG_LEVEL=INFO
UVICORN_WORKERS=2

# Memory
MEMORY_STORAGE_BACKEND=sqlite
MEMORY_STORAGE_PATH=./data/memory

# Context
CONTEXT_MAX_TOKENS=4000
CONTEXT_SUMMARY_ENABLED=true

# Observability
TRACE_DB_PATH=./monitoring/trace.db
TRACE_RETENTION_DAYS=14

# LLM Providers (set at least one)
DASHSCOPE_API_KEY=
QWEN_API_KEY=
ZHIPU_API_KEY=
DEEPSEEK_API_KEY=
OPENAI_API_KEY=
OPENAI_BASE_URL=
OPENAI_MODEL=gpt-4

# LLM Router
LLM_ROUTER_ENABLED=true
LLM_FALLBACK_MODEL=qwen-turbo
LLM_MODEL_INTENT_GATE=qwen-turbo
LLM_MODEL_NPC=qwen-plus
LLM_MODEL_COACH=qwen-max
LLM_MODEL_EVALUATOR=glm-4
LLM_MODEL_RAG=glm-4-flash
LLM_MODEL_COMPLIANCE=qwen-turbo

# Frontend
VITE_API_URL=/api/v1

# LangSmith (optional)
LANGSMITH_API_KEY=
LANGSMITH_PROJECT=salesboost
LANGSMITH_TRACING_ENABLED=true
